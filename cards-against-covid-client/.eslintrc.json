{
    "env": {
        "browser": true
    },
    "rules": {
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off",
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "jsx-a11y/label-has-associated-control": [ 2, {
            "controlComponents": ["TextBox"],
            "depth": 2
          }]      
    }
}